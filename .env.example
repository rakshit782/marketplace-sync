# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/marketplace_sync

# Server Configuration
PORT=3001

# Node Environment
NODE_ENV=development

# CORS Configuration (comma-separated origins)
# ALLOWED_ORIGINS=http://localhost:3000,https://yourdomain.com

# Add your marketplace API credentials below
# These are loaded by the application at runtime

# Amazon SP-API
# AMAZON_ACCESS_KEY=your_access_key_here
# AMAZON_SECRET_KEY=your_secret_key_here
# AMAZON_SELLER_ID=your_seller_id_here
# AMAZON_REGION=us-east-1

# Walmart Marketplace API
# WALMART_CLIENT_ID=your_client_id_here
# WALMART_CLIENT_SECRET=your_client_secret_here
# WALMART_CONSUMER_ID=your_consumer_id_here

# eBay API
# EBAY_APP_ID=your_app_id_here
# EBAY_CERT_ID=your_cert_id_here
# EBAY_DEV_ID=your_dev_id_here

# Etsy API
# ETSY_API_KEY=your_api_key_here
# ETSY_SHARED_SECRET=your_shared_secret_here

# Shopify
# SHOPIFY_API_KEY=your_api_key_here
# SHOPIFY_API_SECRET=your_api_secret_here
# SHOPIFY_SHOP_NAME=your_shop_name_here